# see: http://EditorConfig.org

# end_of_line is unspecified so Windows users can use the repo in crlf mode

root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true


[*.md]
indent_style = space
indent_size = 4
trim_trailing_whitespace = false
